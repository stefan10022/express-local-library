<h1><%= title %></h1>
<ul>
  <% if(author_list.length !== 0){ author_list.forEach(function(author){%>
  <li>
    <a href="<%= author.url %>"><%= author.name %></a> (<%= author.date_of_birth %>
    - <%= author.date_of_death %> )
  </li>
  <% })} else {%>
  <li>There are no authors.</li>
  <% } %>
</ul>
